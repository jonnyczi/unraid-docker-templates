    <Volume>
      <HostDir>/mnt/user/appdata/centos-crontab-php/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>http://php.net/images/logo.php</Icon>
</Containers>