<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2016-08-04</Date>
  <Beta>False</Beta>
  <Category>Web:site</Category>
  <Name>Centos-Dropbox</Name>
  <Overview>
    Dropbox Client
  </Overview>
  <Description>
    Dropbox client running on Centos[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]Tip:[/b] You can add more volume mappings if you wish.
  </Description>
  <Registry>https://registry.hub.docker.com/u/jonnyczi/centos-dropbox/</Registry>
  <GitHub>https://github.com/jonnyczi/centos-dropbox</GitHub>
  <Repository>jonnyczi/centos-dropbox</Repository>
  <Support>http://cedmafinance.ca</Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/centos-dropbox/.dropbox</HostDir>
      <ContainerDir>/root/.dropbox</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/centos-dropbox/Dropbox</HostDir>
      <ContainerDir>/root/Dropbox</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>http://php.net/images/logo.php</Icon>
</Containers>